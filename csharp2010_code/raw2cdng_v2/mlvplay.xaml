<Window x:Class="raw2cdng_v2.mlvplay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="mlvplay" Height="166" Width="150" SizeToContent="WidthAndHeight" KeyUp="Window_KeyUp">
